{
  "name": "soul",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "concurrently --group --timings 'npm run lint:eslint' 'npm run check-types' 'npm run lint:prettier'",
    "lint:eslint": "eslint \"{src,apps,libs,test-e2e,factories,config}/**/*.ts\"",
    "lint:prettier": "prettier --check \"{src,apps,libs,test-e2e,factories,config}/**/*.ts\"",
    "check-types": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:debug": "node --inspect-brk node_modules/.bin/jest --config ./test-e2e/jest-e2e.json --runInBand",
    "test:e2e": "jest --config ./test-e2e/jest-e2e.json --runInBand --forceExit",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.ts",
    "migration:generate": "rm -rf ./dist && ts-node ./node_modules/.bin/typeorm -f ./ormconfig.ts migration:generate",
    "migration:run": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.ts migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.ts migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nicholas.braun/nestjs-redoc": "^2.2.3",
    "@sentry/node": "^7.14.0",
    "@sentry/tracing": "^7.14.0",
    "axios": "^0.27.2",
    "base64url": "^3.0.1",
    "bcrypt": "^5.0.1",
    "bull": "^4.10.0",
    "cache-manager": "^5.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "figlet": "^1.5.2",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "joi": "^17.6.2",
    "mysql2": "^2.3.3",
    "nest-raven": "^9.0.0",
    "nestjs-pino": "^3.1.1",
    "nestjs-throttler-storage-redis": "^0.1.20",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-http": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cron": "^2.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.14",
    "@types/figlet": "^1.5.5",
    "@types/jest": "29.1.1",
    "@types/node": "^18.7.23",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fishery": "^2.2.2",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^factories/(.*)$": "<rootDir>/../factories/$1",
      "^factories$": "<rootDir>/../factories"
    },
    "coveragePathIgnorePatterns": [
      ".module.ts",
      ".exception.ts",
      ".dto.ts",
      ".entity.ts",
      "main.ts",
      "request-log.interceptor.ts"
    ]
  }
}

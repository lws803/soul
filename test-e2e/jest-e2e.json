{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 60000,
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^factories/(.*)$": "<rootDir>/../factories/$1",
    "^factories$": "<rootDir>/../factories",
    "^config$": "<rootDir>/../config"
  }
}
